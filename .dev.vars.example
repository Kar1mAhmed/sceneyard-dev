# NextAuth.js Environment Variables
# Copy this file to .dev.vars for local development

# Required: Secret used to encrypt JWT tokens
# Generate with: npx auth secret
AUTH_SECRET=your-secret-here-generate-with-npx-auth-secret

# Required: Google OAuth credentials
# Get these from: https://console.cloud.google.com/apis/credentials
AUTH_GOOGLE_ID=your-google-client-id
AUTH_GOOGLE_SECRET=your-google-client-secret

# Optional: Base URL for your application
# Defaults to http://localhost:3000 in development
# AUTH_URL=http://localhost:3000
