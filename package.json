{
	"name": "sceneyard-dev",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build --webpack",
		"opennext:build": "npx @opennextjs/cloudflare build",
		"preview": "npm run opennext:build && npx wrangler dev",
		"deploy": "npx wrangler deploy",
		"start": "next start",
		"lint": "next lint",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"migrations-local": "npx wrangler d1 migrations apply sceneyard --local",
		"migrations-prod": "npx wrangler d1 migrations apply sceneyard --remote"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.3.0",
		"@types/uuid": "^10.0.0",
		"next": "^16.0.1",
		"next-auth": "^5.0.0-beta.30",
		"next-devtools-mcp": "^0.2.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"uuid": "^13.0.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251014.0",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.21",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.43.0"
	}
}
